}

#projets li span.bold, #valorisation li span.bold, #parcours li span.bold {
	font-weight: bold;
}

#projets li span.underline, #valorisation li span.underline {
	text-decoration: underline;
}

#projets li img, #parcours li img {
	margin: 30px 10px 30px 15px;
	width: 270px;
	height: 200px;
}

#projets li img.img-center, #parcours li img.img-center {
	position: relative;
	left: 150px;
}

#parcours li img.img-logo {
	width: 270px;
	height: 100%;
	margin-bottom: 100px;
	position: relative;
	left: 150px;
	top: 10px;
}

#parcours li img.img-logo-left {
	width: 270px;
	height: 100%;
	margin-bottom: 100px;
	position: relative;
	right: 0px;
	top: 10px;
}

#parcours li img.img-grd-logo {
	width: 270px;
	height: 100%;
	margin-bottom: 55px;
	position: relative;
	left: 150px;
}

#parcours li img.img-logo-middle {
	width: 270px;
	height: 100%;
	margin-bottom: 100px;
	position: relative;
	left: 0px;
}

#parcours li.circle {
	list-style-type: circle;
	margin-left: 50px;
	line-height: 1.4em;
}

#contact_me p {
	text-align: center;
	color: #333;
}

#contact_me #sandz {
	display: inline-block;
	border: solid 5px #fff;
	margin-top: 150px;
	margin-left: 104px;
	-webkit-box-shadow: 0px 5px 10px #000000;
    -moz-box-shadow: 0px 5px 10px #000000;
    -o-box-shadow: 0px 5px 10px #000000;
	-ms-box-shadow: 0px 5px 10px #000000;
    box-shadow: 0px 5px 10px #000000;  
	-moz-transform: rotate(-10deg);
	-o-transform: rotate(-10deg);
	-webkit-transform: rotate(-10deg);
	-ms-transform: rotate(-10deg);
